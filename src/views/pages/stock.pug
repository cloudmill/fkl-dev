extends ../layouts/master.pug

block pageName
  - var pageName = "stock contactsBlock"

block header
  include ../partials/_header

block content
  .section
    .container
      +mainTitle('Склад', 'Контакты', true)
      +menuSub(menuWhereBuy, 'stock')
    .section__left(data-aos="")
      .stock-info
        .row
          .contacts-list.col.col__md-6.col__xs-12(data-aos="fade-up", data-aos-delay="600")
            .contacts-list__ico
              +img('contacts/pin.svg')
            .contacts-list__desc
              h5 Воронежская область, село #[br] Бабяково, Новоусманского района
              p.text--grey.text__sm ул. Индустриальная, 61в
          .contacts-list.col.col__md-6.col__xs-12(data-aos="fade-right", data-aos-delay="700")
            .contacts-list__ico
              +img('contacts/gps.svg')
            .contacts-list__desc
              h5 GPS: 51.707907, 39.455410 #[br] Индустриальный Парк «ПЕРСПЕКТИВА»
              p.text--grey.text__sm 10 км от г. Воронежа съезд с М-4 на Тамбов (А-144)
      #googleMap.googleMap(data-aos="fade-right", data-aos-delay="800")
      +nextBlock('Контакты дилеров')
      include ../components/_signForm

block footer
  .section__footer
    include ../partials/_footer

